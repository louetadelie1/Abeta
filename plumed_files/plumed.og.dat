MOLINFO MOLTYPE=protein STRUCTURE=../template.pdb 
WHOLEMOLECULES ENTITY0=1-627

# print detailed (action-by-action) timers at the end of simulation
#DEBUG DETAILED_TIMERS FILE=DEBUG

# EEF1SB Implicit solvation
protein-h: GROUP NDX_FILE=../index.ndx NDX_GROUP=Protein-H 
solv: EEFSOLV ATOMS=protein-h NOPBC NL_STRIDE=20 NL_BUFFER=0.1 
bias: BIASVALUE ARG=solv 

#include file with definition of metadynamics CVs
INCLUDE FILE=../ticA.dat

#include file with definition of metadynamics CVs involving the small molecule
INCLUDE FILE=../CVs_drug.dat

